{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "name": "Category",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "products": {
      "via": "category",
      "collection": "products"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 74,
      "minLength": 10
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
